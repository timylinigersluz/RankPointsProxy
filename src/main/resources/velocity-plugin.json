{
  "id": "rankproxyplugin",
  "name": "RankProxyPlugin",
  "version": "1.0",
  "authors": ["ksrminecraft tecs"],
  "main": "ch.ksrminecraft.RankProxyPlugin.RankProxyPlugin",

  "dependencies": [
    {
      "id": "luckperms",
      "optional": false
    }
  ],

  "permissions": {
    "rankproxyplugin.addpoints": {
      "description": "Erlaubt das Verwenden von /addpoints"
    },
    "rankproxyplugin.setpoints": {
      "description": "Erlaubt das Verwenden von /setpoints"
    },
    "rankproxyplugin.getpoints": {
      "description": "Erlaubt das Verwenden von /getpoints"
    },
    "rankproxyplugin.reloadconfig": {
      "description": "Erlaubt das Neuladen der Plugin-Konfiguration (/reloadconfig)"
    },
    "rankproxyplugin.staff.add": {
      "description": "Erlaubt das Hinzufügen eines Spielers zur Stafflist"
    },
    "rankproxyplugin.staff.remove": {
      "description": "Erlaubt das Entfernen eines Spielers von der Stafflist"
    },
    "rankproxyplugin.staff.list": {
      "description": "Erlaubt das Auflisten der Stafflist"
    },
    "rankproxyplugin.rankinfo": {
      "description": "Erlaubt die Abfrage von Ranginformationen über /rankinfo"
    }
  }
}
